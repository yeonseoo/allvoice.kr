@CHARSET "UTF-8";

body{font-family: 'PT Sans','Noto Sans KR', sans-serif;background-color:#f4f4f4;}
table{border-spacing:0;}

.slcWrap{position:relative;display:inline-block;width:100%;}
.orderForm > ul > li .slcWrap{width:calc(100% - 80px);}
.slcWrap select{-webkit-appearance: none;-moz-appearance: none;position:relative;background:transparent;z-index:20;}
.slcWrap select::-ms-expand {
    display: none;
}
.slcWrap::after{
	width:28px;
	height:28px;
	background:url("../img/bg_dropDown.png") no-repeat;
	content:" ";
	display:block;
	position:absolute;top:10px;right:10px;
	z-index:10;
}

.orderForm .slcWrap::after{
	top:1px;
	right:1px;
}


body{padding-top:71px;}
#ttrxHeader{position:fixed;top:0;left:0;width:100vw;background-color:#f4f4f4;z-index:3000;border-bottom:1px #e4e4e4 solid;}
#ttrxHeader > div{padding:20px 15px;overflow:hidden;}
#ttrxHeader > div > a{float:left;display:block;}
#ttrxHeader > div > a img{width:135px;}
#ttrxHeader > div > div{overflow:hidden;float:right;}
#ttrxHeader > div > div > a{float:left;display:block;}
#ttrxHeader > div > div > a > img{width:28px;}
#ttrxHeader > div > div > a > img:nth-child(1){display:block;}
#ttrxHeader > div > div > a > img:nth-child(2){display:none;}
#ttrxHeader > div > div > a.on > img:nth-child(1){display:none;}
#ttrxHeader > div > div > a.on > img:nth-child(2){display:block;}

#searchPop{position:absolute;padding:25px 15px 50px 15px;background-color:#f4f4f4;width:100%;top:65px;left:0;border-top:1px #8a8a8a solid;display:none;z-index:5000;}
#searchPop > strong{display:block;font-size:20px;color:#333333;padding-bottom:17px;font-weight:normal;}
#searchPop form{overflow:hidden;}
#searchPop input[type="text"]{float:left;border:1px #a0a0a0 solid;border-right:0;height:34px;padding:0 8px;width:calc(100% - 35px);}
#searchPop input[type="image"]{width:35px;border:1px #a0a0a0 solid;float:left;}


#gnbPop{display:none;height:calc(100vh - 71px);overflow:scroll;}
#gnbPop > div{overflow:hidden;background-color:#084162;border-top:1px #59859f solid;border-bottom:1px #59859f solid;}
#gnbPop > div > a{display:block;width:25%;float:left;padding:50px 0 10px 0;text-align:center;color:#fff;border-left:1px #59859f solid;}
#gnbPop > div > a:first-child{border-left:0;}
#gnbPop > div > a:nth-child(1){background:url("../img/mobile/bg_service01.png") no-repeat;background-size:auto 25px;background-position:center 15px;}
#gnbPop > div > a:nth-child(2){background:url("../img/mobile/bg_service02.png") no-repeat;background-size:auto 25px;background-position:center 15px;}
#gnbPop > div > a:nth-child(3){background:url("../img/mobile/bg_service03.png") no-repeat;background-size:auto 25px;background-position:center 15px;}
#gnbPop > div > a:nth-child(4){background:url("../img/mobile/bg_service04.png") no-repeat;background-size:auto 25px;background-position:center 15px;}
#gnbPop > div > a.on{background:url("../img/mobile/bg_service05.png") no-repeat;background-size:auto 25px;background-position:center 15px;}
#gnbPop > ul{padding:5px 15px;}
#gnbPop > ul > li{}
#gnbPop > ul > li > a{line-height:45px;display:block;font-size:20px;color:#4d4d4d;border-bottom:1px #a0a0a0 dashed;padding:0 10px;}
#gnbPop > ul > li > a.drop{background:url("../img/mobile/btn_gnbDown.png") no-repeat right 10px center;background-size:10px;}
#gnbPop > ul > li > a.drop.on{background:url("../img/mobile/btn_gnbUp.png") no-repeat right 10px center;background-size:10px;color:#4e9bbc;font-weight:bold;border-bottom:1px #333 solid;;}
#gnbPop > ul > li > ul{border-bottom:1px #333333 solid;margin-top:-1px;}
#gnbPop > ul > li > ul > li{border-bottom:1px #d2d2d2 dashed;}
#gnbPop > ul > li > ul > li:last-child{border-bottom:0;}
#gnbPop > ul > li > ul > li > a{font-size:15px;color:#727272;padding:0 10px;line-height:35px;}


#ttrxFooter{border-top:1px #b9babc solid;background-color:#ececec;margin-top:45px;}
#ttrxFooter.on{margin-bottom:78px;}
#ttrxFooter > div{border-bottom:1px #b9babc solid;text-align:center;padding:10px 0;}
#ttrxFooter > div:first-child{padding:15px 0;}
#ttrxFooter > div a{color:#777777;font-size:11px;}
#ttrxFooter > div img{margin:0 7px;width:60px;}
#ttrxFooter > p{padding:20px 0;text-align:center;color:#777777;font-size:11px;line-height:18px;}
#ttrxFooter > p img{width:138px;margin:0 auto 5px auto;display:block;}
#ttrxFooter > p span{color:#999999;}

#ttrxFooter > div .cs_center a{
  font-weight: bold;
  color: #084162;
}
















