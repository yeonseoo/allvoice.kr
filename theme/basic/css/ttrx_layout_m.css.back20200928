.pop-container .pop-btn-wrap a {border:1px solid #5f6edc;color:#5f6edc;font-size:16px;padding:10px 20px;margin:0 2px;width:40%;display:inline-block;overflow:hidden;position:relative;}
.pop-container .pop-btn-wrap a.blue-btn {border:1px solid #5f6edc;color:#fff;background:#5f6edc;}
.pop-container .popup_scorll {border-top:1px solid #5f6edc;border-bottom:1px solid #5f6edc;height:200px;overflow-y:scroll;padding:0 15px;text-align:left;}
.pop-container .popup_scorll::-webkit-scrollbar { -webkit-appearance: none; }
.pop-container .popup_scorll::-webkit-scrollbar:vertical { width: 12px; }
.pop-container .popup_scorll::-webkit-scrollbar:horizontal { height: 12px; }
.pop-container .popup_scorll::-webkit-scrollbar-thumb { background-color: rgba(0, 0, 0, .5); border-radius: 10px; border: 2px solid #ffffff; }
.pop-container .popup_scorll::-webkit-scrollbar-track { border-radius: 10px; background-color: #ffffff; }
.pop-container .popup_scorll .sec_popup {margin:10px 0;border-bottom:1px solid #ececec;padding:10px 0;}
.pop-container .popup_scorll .sec_popup p {margin-bottom:5px;}
.pop-container .popup_scorll .sec_popup h4 {color:#222;font-weight:600;margin-bottom:5px;font-size:17px;}
.pop-container .popup_scorll .sec_popup:last-child {border-bottom:none;}
.pop-container .input_wrap {margin:30px auto;width:100%;overflow:hidden;text-align:center;vertical-align:middle;}
.pop-container .input_wrap input {line-height:40px;height:40px;padding:0 10px;border-radius:5px;border:1px solid #dfdfdf;text-align:left;vertical-align:middle;}
.pop-container .input_wrap select {margin:0;
	border-radius:0; /* 아이폰 사파리 보더 없애기 */
	-webkit-appearance:none; /* 화살표 없애기 for chrome*/
	-moz-appearance:none; /* 화살표 없애기 for firefox*/
	appearance:none;
	background:url(/img/select_arrow.svg) no-repeat 90% 50%;min-width:42%;line-height:40px;height:40px;border: solid 1px #dfdfdf;border-radius:5px; padding:0 5px;font-family: 'PT Sans','Noto Sans KR', sans-serif;vertical-align:middle;}
.pop-container .input_wrap select::-ms-expand {   display: none;            /* 화살표 없애기 for IE10, 11*/}
.pop-btn-close {position:absolute;right:-5px;top:-10px;}
a.btn-layerClose {

  color: #979797;
  font-size:20px;
}

a.btn-layerClose:hover {
  color: #4e9bbc;
}


input[type="radio"].invisible-radio {
    display:none;
}

input[type="radio"].invisible-radio + label {
    color:#666;
}

input[type="radio"].invisible-radio + label span {
    display:inline-block;
    background:url(/img/check.png) no-repeat 0% 50%;margin:0 1px;vertical-align:middle;height:17px;line-height:17px;
    cursor:pointer;
	padding-left:20px;

}

input[type="radio"].invisible-radio:checked + label span {
  background:url(/img/checked.png) no-repeat 0% 50%;vertical-align:middle;height:17px;padding-left:20px;line-height:17px;
}
.contest_title_wrap {padding:30px 0px;text-align:center;}
.contest_title_wrap h4 {line-height: 30px;    color: #222;    font-size: 23px;margin-bottom:10px;}
.contest_title_wrap p {line-height: 22px;    color: #666;font-size:17px;}
.contest_title_wrap p.date {margin-top:20px;}
.contest_title_wrap p.date i {color:#4e9bbc;font-size:17px;}
.contest_title_wrap p.date strong.color {color:#4e9bbc;}


.contest_sec_wrap .tagPlayer > div:nth-child(1) strong {
    margin-bottom: 7px;
}
.contest_sec_wrap .tagPlayer > div:nth-child(1) strong font {
   font-weight:300;color:#ddd;
}

.listContest{margin: 0;padding:30px 15px 0 15px;}
.listContest ul{overflow:hidden;}
.listContest ul li{float:left;width:50%;}
.listContest ul li a{padding:0 20px;line-height:50px;font-size:15px;color:#b1b1b1;background:#eeeeee;display:block;text-align:center;}
.listContest ul li a.on,
.listContest ul li a:hover{background:#5f6edc;;color:#fff;}
.contest_sec_wrap .Player_wrap {padding:15px 0 ;}
.contest_sec_wrap .player_name {font-size:18px;color:#222222;margin-bottom:10px;vertical-align:middle;}
.contest_sec_wrap .player_name img {vertical-align:middle;}
.contest_sec_wrap .player_name span {background:#ff1382;display:inline-block;padding:4px 16px;color:#fff;font-size:12px;border-radius:10px;line-height:12px;margin-right:5px ;}
