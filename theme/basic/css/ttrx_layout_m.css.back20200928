.voiceProfile > div > b span{font-weight:normal;color:#666666;display:block;font-size:13px;}
.voiceProfile > div > span{font-size:13px;display:block;text-align:center;color:#666666;line-height:18px;margin-bottom:5px;}
.voiceProfile > div > span img{vertical-align:middle;margin-right:3px;}
.voiceProfile > div > div{text-align:center;}
.voiceProfile > div > div b{display:inline-block;margin-right:5px;color:#666666;font-size:11.5px;}
.voiceProfile > div > div *{vertical-align:middle;}
