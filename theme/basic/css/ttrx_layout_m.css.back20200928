.voiceProfile{}
.voiceProfile > div{position:relative;background:#fff url("../img/mobile/bg_profileLine.png") no-repeat center center;border:1px #dddddd solid;border-radius:10px;margin:0 15px;padding:20px 12px 20px 50%;min-height:230px;text-align:center;}
.voiceProfile > div > img:nth-child(1){width:135px;height:135px;border-radius:94px;border:2px #fff solid;box-shadow: 0 0 0 2px #ddd;position:absolute;left:12px;top:50%;transform:translateY(calc(-50% - 20px));}
.voiceProfile > div > img:nth-child(2){position:absolute;left:115px;top:50%;transform:translateY(calc(-50% + 25px));}
.voiceProfile > div > strong{font-size:16px;color:#1b2329;display:block;text-align:center;width:135px;position:absolute;left:12.5px;top:170px;}
.voiceProfile > div > i{font-style:normal;display:inline-block;line-height:23px;font-size:14px;color:#4e9bbc;border:1px #4e9bbc solid;border-radius:11.5px;padding:0 11px;margin-bottom:10px;margin-top:35px;}
