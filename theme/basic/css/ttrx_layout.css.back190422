.voiceDetailTab ul li:hover + li a{border-left-color:#ebebeb;}
.voiceDetailTab ul li.on + li:hover a{border-left-color:#fff;}


.voiceProfile{width:400px;float:right;background-color:#fff;}
.voiceProfile > div{text-align:center;position:relative;padding-top:30px;}
.voiceProfile > div > img:nth-child(1){display:block;margin:2px auto;width:188px;height:188px;border-radius:94px;border:2px #fff solid;box-shadow: 0 0 0 2px #ddd;}
.voiceProfile > div > img:nth-child(2){position:absolute;left:244px;top:158px;}
.voiceProfile > div > strong{font-size:33px;color:#1b2329;display:block;text-align:center;line-height:65px;}
.voiceProfile > div > i{font-style:normal;display:inline-block;line-height:23px;font-size:15px;color:#4e9bbc;border:1px #4e9bbc solid;border-radius:11.5px;padding:0 11px;margin-bottom:10px;}
.voiceProfile > div > b{display:block;text-align:center;font-size:15px;color:#666666;margin-bottom:6px;}
.voiceProfile > div > b span{font-weight:normal;margin-left:5px;color:#666666;}
.voiceProfile > div > span{font-size:15px;display:block;text-align:center;color:#666666;line-height:18px;margin-bottom:5px;}
.voiceProfile > div > span img{margin-top:-2px;}
.voiceProfile > div > div{text-align:center;}
.voiceProfile > div > div b{display:inline-block;margin-right:5px;color:#666666;font-size:15px;}
.voiceProfile > div > div *{vertical-align:middle;}
.voiceProfile > div > ul{overflow:hidden;border-bottom:1px #dddddd dashed;border-top:1px #dddddd dashed;width:340px;margin:10px auto 30px auto;padding:15px 10px;}
.voiceProfile > div > ul > li{font-size:15px;padding:0 4px;}
.voiceProfile > div > ul > li:nth-child(2n+1){float:left;clear:both;color:#333333;}
.voiceProfile > div > ul > li:nth-child(2n){float:right;color:#777777;}
.voiceProfile > div > a{display:block;margin:0 auto;margin-bottom:30px;text-align:center;width:260px;line-height:50px;font-size:18px;color:#fff;border-radius:5px;background-color:#4e9bbc;}
.voiceProfile > ul{width:340px;margin:30px auto;padding:20px 0;;border-bottom:1px #dddddd dashed;border-top:1px #dddddd dashed;text-align:center;}
.voiceProfile > ul li{display:inline-block;margin:0 10px;}
.voiceProfile > hr{display:block;width:340px;margin:20px auto;border:0;border-bottom:1px #dddddd dashed;}
.voiceProfile > a.sendAgree{display:block;margin:0 auto;margin-bottom:13px;text-align:center;width:260px;line-height:50px;font-size:18px;color:#fff;border-radius:5px;background-color:#b01217;}
.voiceProfile > a.sendMassage{display:block;margin:0 auto;margin-bottom:30px;text-align:center;width:260px;line-height:50px;font-size:18px;color:#fff;border-radius:5px;background-color:#084162;}
.voiceProfile > a img{margin-top:-3px;margin-right:3px;}
.voiceProfile > section{border-top:1px #dddddd solid;padding:25px 30px;}
.voiceProfile > section ul li{border-bottom:1px #dddddd dashed;}
.voiceProfile > section ul li:last-child{border-bottom:0;}
.voiceProfile > section ul li a{color:#333333;font-size:18px;line-height:58px;padding-left:10px;}
.voiceProfile > article{padding:30px 30px 0 30px;}
.voiceProfile > article > span{display:block;font-size:14px;}
.voiceProfile > article > strong{display:block;font-size:20px;}
.voiceProfile > article > ol{margin-top:10px;}
.voiceProfile > article > ol > li{list-style:none;font-size:14px;margin-top:5px;}

.voiceDetailProfile{}
.voiceDetailProfile > strong{color:#222222;font-size:24px;font-weight:normal;}
.voiceDetailProfile > span{color:#222222;font-size:18px;font-weight:normal;display:block;}
.voiceDetailProfile > hr{display:block !important;border:0;border-bottom:1px #dddddd solid;margin:28px 0 13px 0;}
.voiceDetailProfile > p{color:#777777;font-size:15px;margin-bottom:25px;padding-top:15px;}
.voiceDetailProfile > p span{color:#222222;}
.voiceDetailProfile > p i{font-weight:bold;color:#084162;font-style:normal;}
.pdfDown{overflow:hidden;padding-top:15px;}
.pdfDown span{float:left;font-size:16px;display:block;line-height:45px;}
.pdfDown a{float:right;}
.projectApply{overflow:hidden;padding-top:15px;}
.projectApply span{float:left;font-size:28px;display:block;line-height:50px;font-weight:bold;}
.projectApply div{float:right;margin-top:0;}

.voiceSample{width:100%;}
.voiceSample th{height:49px;font-weight:bold;color:#aaaaaa;font-size:15px;}
.voiceSample tbody tr:nth-child(2n+1){background-color:#f7f7f7;}
.voiceSample tbody tr:nth-child(2n+1) td{text-align:center;color:#333333;font-size:15px;height:84px;}
.downloadBtn,
