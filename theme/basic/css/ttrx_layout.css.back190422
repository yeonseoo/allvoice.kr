@CHARSET "UTF-8";


#fregister_private textarea{resize: none;overflow:auto;}
#fregister_private textarea:focus{border:0 !important;margin:0 !important;box-shadow:none !important;}

body{font-family: 'PT Sans','Noto Sans KR', sans-serif;padding-top:106px;}
table{border-spacing:0;}

.slcWrap{position:relative;display:inline-block;width:100%;}
.orderForm > ul > li .slcWrap{width:calc(100% - 100px);} 
.slcWrap select{-webkit-appearance: none;-moz-appearance: none;position:relative;background:transparent;z-index:20;}
.slcWrap select::-ms-expand {
    display: none;
}
.slcWrap::after{
	width:28px;
	height:28px;
	background:url("../img/bg_dropDown.png") no-repeat;
	content:" ";
	display:block;
	position:absolute;top:10px;right:10px;
	z-index:10;
}
.datepicker{z-index:10000 !important;}

.ttrxHeader{background-color:#f4f4f4;position:fixed;top:0;left:0;width:100%;z-index:90;}
.ttrxHeader > div{overflow:hidden;padding:34px;position:relative;width:1320px;margin:0 auto;}
.ttrxHeader > div > a{position:absolute;top:50%;left:0;transform:translateY(-50%);}
.ttrxHeader > div > div{float:right;}
.ttrxSearchEngine{overflow:hidden;border:1px #231815 solid;;float:left;}
.ttrxSearchEngine input{float:left;}
.ttrxSearchEngine input[type="text"]{border:0;padding-left:10px;height:36px;padding:8px;background-color:#f4f4f4;color:#443b38;}
.ttrxSearchEngine input[type="text"]:focus{border:0 !important;outline:0 !important;}
.ttrxHeader > div > div > span{display:block;overflow:hidden;float:left;}
.ttrxHeader > div > div > span a{float:left;line-height:38px;margin-left:10px;padding:0 25px;font-size:14px;}
.ttrxHeader > div > div > span a:first-child{color:#2b2c2f;background-color:#dddede;}
.ttrxHeader > div > div > span a:last-child{color:#ffffff;background-color:#b01217;}
.ttrxHeader > div > div > ul{float:left;overflow:hidden;margin-left:5px;}
.ttrxHeader > div > div > ul > li{float:left;margin-left:25px;}
.ttrxHeader > div > div > ul > li > a{line-height:38px;display:block;color:#231815;font-size:13px;}

.ttrxHeader.sub{background-color:#084162;position:fixed;top:0;left:0;width:100%;z-index:100;}
.ttrxHeader.sub .ttrxSearchEngine input[type="text"]{background-color:#144a69;color:#fff;}
.ttrxHeader.sub > div > div > ul > li > a{color:#fff;}

#mainBx{position:relative;}
#mainBx li{width:100%;height:434px;position:relative;overflow:hidden;}
#mainBx li img{position:absolute;top:0;left:50%;transform:translateX(-50%);width:1920px;}
#mainBx li div{width:1320px;height:434px;position:absolute;left:50%;bottom:0;transform:translateX(-50%);}
#mainBx li div a:nth-child(1){position:absolute;bottom:96px;left:60px;width:145px;line-height:48px;color:#ffffff;text-align:center;background-color:#4e9bbc;font-size:16px;border:1px #4e9bbc solid;}
#mainBx li div a:nth-child(2){position:absolute;bottom:96px;left:214px;width:145px;line-height:48px;color:#1c7196;text-align:center;background-color:transparent;font-size:16px;border:1px #4e9bbc solid;}
#mainBx .bx-prev{text-indent:-100000px;width:10px;height:21px;display:block;background:url("../img/btn_prev.png") no-repeat;position:absolute;top:180px;left:calc(50% - 660px);}
#mainBx .bx-next{text-indent:-100000px;width:10px;height:21px;display:block;background:url("../img/btn_next.png") no-repeat;position:absolute;top:180px;right:calc(50% - 660px);}

#mainCate{background-color:#084162;padding:55px 0 5px 0;}
#mainCate > ul{overflow:hidden;width:1320px;margin:0 auto;}
#mainCate > ul > li{margin-bottom:35px;float:left;width:calc(100% / 6);}
#mainCate > ul > li a{display:block;text-align:center;}
#mainCate > ul > li a > img{display:block;margin:0 auto;}
#mainCate > ul > li a > img:nth-child(2){display:none;}

#mainCate > ul > li a:hover > img:nth-child(1){display:none;}
#mainCate > ul > li a:hover > img:nth-child(2){display:block;}

#mainCate > ul > li a span{display:block;text-align:center;font-size:14px;color:#fff;padding-top:12px;}
#mainCate > ul > li a:hover span{color:#97e0ff;}

#topTen{background-color:#f7f7f7;padding:100px 0 95px 0;text-align:center;}
#topTen > strong{display:block;text-align:center;padding-bottom:75px;color:#1b2329;font-size:40px;}
#topTen > ul{display:inline-block;width:1320px;margin:0 auto;}
#topTen > ul li{float:left;margin-bottom:50px;width:calc(20% - 24px);margin-left:30px;position:relative;}
#topTen > ul li:nth-child(5n+1){margin-left:0;}
#topTen > ul li > div > img{width:100%;height:240px;border-radius:120px;margin:2px;border: 2px #fff solid;box-shadow: 0 0 0 2px #ddd;}
#topTen > ul li > a{padding-top:15px;display:block;text-align:center;color:#666666;font-size:16px;}



#mainAbout{padding:40px 0 35px 0;background-color:#4e9bbc;text-align:center;}
#mainAbout p{color:#fff;font-size:18px;line-height:30px;padding:20px 0;}

.more01{display:inline-block;border:1px #ffffff solid;padding:0 25px;line-height:36px;color:#fff;}
.more01 img{margin-left:5px;vertical-align:middle;}
.more02{display:inline-block;border:1px #084162 solid;padding:0 25px;line-height:36px;color:#fff;background-color:#084162;}
.more02 img{margin-left:5px;vertical-align:middle;}
.more03{text-align:center;}
.more03 a{display:inline-block;padding:0 40px 0 25px;line-height:38px;color:#fff;font-size:14px;background:url("../img/bg_more.png") no-repeat;background-color:#084162;background-position: top right 23px;}

#workList{background-color:#f7f7f7;padding:90px 0 95px 0;text-align:center;}
#workList > strong{display:block;text-align:center;padding-bottom:10px;color:#444444;font-size:40px;}
#workList > span{display:block;text-align:center;padding-bottom:70px;color:#444444;font-size:22px;}
.projectList{overflow:hidden;width:1320px;margin:0 auto;padding-bottom:60px;}
.listWrap.project .listCont{padding:10px 0 50px 0;}
.project .projectList{width:1320px;}
.projectList li{float:left;width:calc(25% - 21px);margin-left:28px;margin-bottom:28px;background-color:#fff;border-radius:20px;overflow:hidden;}
.projectList li:nth-child(4n+1){margin-left:0;}
.projectList li > a{display:block;text-align:center;padding-bottom:20px;}
.projectList li > a > img{width:100%;margin-bottom:30px;}
.projectList li > a > strong{display:block;text-align:center;font-weight:normal;color:#4e9bbc;font-size:18px;}
.projectList li > a > i{width:270px;margin:0 auto;display:block;text-align:center;font-style:normal;color:#222222;font-size:18px;padding-bottom:10px;}
.projectList li > a > em{width:270px;margin:0 auto;display:block;text-align:center;font-style:normal;color:#2b2c2f;font-size:26px;padding-bottom:15px;}
.projectList li > a > em > span{font-size:16px;margin-left:3px;}
.projectList li > a > span{width:270px;margin:0 auto;display:block;text-align:left;font-size:15px;color:#777777;padding-top:13px;border-top:1px #dddddd solid;padding-bottom:8px;}
.projectList li > a > b{width:270px;margin:0 auto;display:block;text-align:left;color:#888888;font-size:14px;padding-left:25px;line-height:18px;background:url("../img/img_chk.png") no-repeat;font-weight:normal;margin-bottom:5px;}


#ttrxFooter{background-color:#ececec;padding:45px 0;}
#ttrxFooter > div{width:1320px;margin:0 auto;position:relative;}
#ttrxFooter > div > img{position:absolute;top:50%;transform:translateY(-50%);left:0;}
#ttrxFooterInfo{margin-left:87px;}
#ttrxFooterInfo p{color:#777;font-size:12px;}
#ttrxFooterInfo span{padding-top:12px;color:#999999;display:inline-block;}
#ttrxFooterEtc{position:absolute;top:50%;transform:translateY(-50%);right:0;}
#ttrxFooterEtc ul{overflow:hidden;float:right;}
#ttrxFooterEtc ul li{float:left;margin-left:15px;}
#ttrxFooterEtc div{clear:both;padding-top:10px;color:#999999;}
#ttrxFooterEtc div a{color:#999999;}


#contentsWrap{background-color:#f7f7f7;padding-bottom:65px;overflow:hidden;margin-top:57px;min-height:calc(100vh - 316px);}
#contentsWrap > div{width:1320px;margin:0 auto;}

#subCate{border-bottom:1px #c1c1c1 solid;position:fixed;top:106px;left:0;width:100%;z-index:90;}
#subCateDep01{background-color:#4e9bbc;}
#subCateDep01 > div{width:1320px;margin:0 auto;}
#subCateDep01 > div > ul{display:inline-block;}
#subCateDep01 > div > ul > li{display:inline-block;}
@media screen and (min-width:0\0){
	#subCateDep01 > div > ul > li{margin-left:-5px;}
	#subCateDep01 > div > ul > li:first-child{margin-left:0;}
}



#subCateDep01 > div a{display:inline-block;padding:20px 19px;font-size:15px;color:#fff;margin:0;line-height:16px;}
#subCateDep01 > div a.on,
#subCateDep01 > div a:hover{background:url("../img/bg_thisCate.png") no-repeat top center;font-weight:bold;}

#subCateDep01 > div span{border-left:1px #fff solid;border-right:1px #fff solid;display:inline-block;margin:20px 0;}
#subCateDep01 > div span a{margin-top:-23px;display:block;padding-bottom:0;}
.subCateDep02{background-color:#f7f7f7;height:59px;position:fixed;top:163px;left:0;width:100%;border-bottom:1px #c1c1c1 solid;}
.subCateDep02 > div{width:1320px;margin:0 auto;}
.subCateDep02 > div > ul{}
.subCateDep02 > div > ul > li{display:inline-block;}
.subCateDep02 > div > ul > li a{display:block;line-height:58px;border-bottom:1px #c1c1c1 solid;padding:0 8px;margin:0 33.8px 0 11px;font-size:15px;color:#666666;}
.subCateDep02 > div > ul > li:last-child a{margin-right:0;}
@media screen and (min-width:0\0){
	.subCateDep02 > div > ul > li a{margin-right:30px;}
}
.subCateDep02 > div > ul > li a.on,
.subCateDep02 > div > ul > li a:hover{color:#333333;border-bottom:1px #222222 solid;}










.listWrap{overflow:hidden;padding-top:40px;}
.listCont{width:880px;float:left;background-color:#fff;padding:10px 50px 50px 50px;}
.listWrap.project .listCont{width:100%;background:none;margin-top:59px;}
.listInfo{overflow:hidden;border-bottom:1px #dddddd solid;margin-bottom:30px;}
.listLocation{float:left;}
.listLocation ul{overflow:hidden;margin-top:16px;}
.listLocation ul li{float:left;padding-right:16px;margin-right:10px;font-size:15px;line-height:16px;background:url("../img/img_locationSign.png") no-repeat center right;color:#aaaaaa;}
.listLocation ul li:last-child{background:none;}

.listOrder{float:right;}
.listOrder ul{overflow:hidden;}
.listOrder ul li{float:left;}
.listOrder ul li a{padding:0 20px;line-height:50px;font-size:14px;color:#999999;border-bottom:2px rgba(0,0,0,0) solid;display:block;}
.listOrder ul li a.on,
.listOrder ul li a:hover{border-bottom:2px #4e9bbc solid;color:#333333;}

.detailSearch{width:400px;float:right;background-color:#fff;}
.detailSearch > strong{font-size:32px;color:#1b2329;display:block;text-align:center;line-height:65px;padding-bottom:10px;margin-top:10px;}
.detailSearch > em{display:block;margin:0 auto;width:30px;height:5px;background-color:#4e9bbc;margin-bottom:30px;}
.detailSearch > div{width:340px;margin:0 auto;padding:0 40px 10px 40px;margin-bottom:20px;border-bottom:1px #dddddd dashed;}
.detailSearch > div select{display:block;border:1px #dddddd solid;height:50px;border-radius:5px;color:#333333;font-size:18px;width:100%;margin-bottom:10px;padding:0 20px;}
.detailSearch > a{display:block;margin:0 auto;margin-bottom:50px;text-align:center;width:260px;line-height:50px;font-size:18px;color:#fff;border-radius:5px;background-color:#4e9bbc;}





.listData{overflow:hidden;}
.listData li{width:calc(25% - 12.75px);margin-left:17px;margin-bottom:45px;float:left;}
.listData li:nth-child(4n+1){margin-left:0;}
.listData li a{display:block;text-align:center;font-size:16px;color:#666666;}
.listData li a img{width:100%;border: 2px #fff solid;box-shadow: 0 0 0 2px #ddd;overflow:hidden;border-radius:130px;margin:2px 2px 15px 2px;min-height:231px;}
.listData li a.downloadBtn{color:#fff;padding:0 8px;width:100px;margin:10px auto 0 auto;}

.listData2{}
.listData2 > li{overflow:hidden;border-bottom:1px #cdcdcd solid;padding-bottom:20px;margin-bottom:20px;}
.listData2 > li > a{display:block;float:left;margin:2px 25px 2px 2px;border-radius:82px;border: 2px #fff solid;box-shadow: 0 0 0 2px #ddd;overflow:hidden;}
.listData2 > li > a > img{width:164px;height:164px;}
.listData2 > li > div > strong{font-size:18px;line-height:46px;color:#666666;margin-right:20px;display:inline-block;vertical-align:middle;margin-top:-13px;}
.listData2 > li > div > img{width:100px;margin-top:-16px;}
.listData2 > li > div > .tagPlayer{margin-bottom:0;}

.ttrxPaging{padding-bottom:50px;text-align:center;padding-top:10px;}
.ttrxPaging > a{display:inline-block;border:1px #dddddd solid;vertical-align:top;}
.ttrxPaging > ul{display:inline-block;vertical-align:top;margin:0 10px;}
.ttrxPaging > ul > li{float:left;margin-left:-1px;}
.ttrxPaging > ul > li:first-child{margin-left:0;}
.ttrxPaging > ul > li > a{width:33px;line-height:33px;text-align:center;color:#666666;font-size:14px;display:block;border:1px #ddd solid;background-color:#fff;}
