.ttrxPaging > ul > li > a.on{border:1px #4e9bbc solid;background-color:#4e9bbc;color:#fff;}

.ttrxPaging strong{width:33px;max-width:33px;line-height:33px;height:35px;border-radius:0;text-align:center;color:#666666;font-size:14px;display:inline-block;border:1px #ddd solid;background-color:#fff;margin-left:-1px;vertical-align:top;}
.ttrxPaging a{width:33px;line-height:33px;text-align:center;color:#666666;font-size:14px;display:inline-block;border:1px #ddd solid;background-color:#fff;margin-left:-1px;vertical-align:top;}
.ttrxPaging a:hover,
.ttrxPaging a.on{border:1px #4e9bbc solid;background-color:#4e9bbc;color:#fff;}


