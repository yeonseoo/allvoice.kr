.voiceDetail{padding-top:40px;overflow:hidden;}
.voiceDetailInfo{float:left;width:880px;}
.voiceDetailSection{background-color:#fff;margin-bottom:40px;}

.voiceDetailTab{}
/* .voiceDetailTab > a{position:relative;top:-190px;} */
.voiceDetailTab ul{overflow:hidden;}
.voiceDetailTab ul li{float:left;width:20%;padding:20px 0;background-color:#ebebeb;}
.voiceDetailTab.tab3 ul li{width:calc(100% / 3);}
.voiceDetailTab.tab2 ul li{width:calc(100% / 2);}
.voiceDetailTab ul li a{display:block;text-align:center;color:#999999;font-size:18px;border-left:1px #999999 solid;line-height:18px;}
.voiceDetailTab ul li:first-child a{border-left:0;}
.voiceDetailTab + div{padding:60px 50px;}

.voiceDetailTab ul li.on,
.voiceDetailTab ul li:hover{background-color:#fff;}
.voiceDetailTab ul li.on a,
.voiceDetailTab ul li:hover a{color:#333333;border-left-color:#fff;}
