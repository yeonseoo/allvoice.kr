.listData3 li{float:left;width:180px;margin-left:20px;text-align:center;}
.listData3 li:nth-child(4n+1){margin-left:0px;}
.listData3 .mediPlayer{width:180px;height:180px;}
.listData3 .circularPlayerWrap > a{display: block;text-align: center;color: #666666;font-size: 16px;}
.listData3 .circularPlayerWrap > a img{width:176px;height:176px;box-shadow: 0 0 0 2px #ddd;overflow:hidden;border-radius:130px;margin:2px 2px 15px 2px;}
.listData3 li > a{margin:10px auto 0 auto;}

.blueBtn{display:inline-block;border:1px #4e9bbc solid;color:#4e9bbc;line-height:36px;text-align:center;width:156px;font-size:15px;}